{
  "name": "kagurajs",
  "version": "0.2.7",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.iife.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "browser": "./dist/index.iife.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc && node type-to-dist.js",
    "lint": "eslint src/**/* playground/**/* --ext .js,.ts",
    "lint:fix": "eslint src/**/* --ext .js,.ts --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "pnpm run typedoc-build && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "typedoc-build": "tsc && typedoc --plugin typedoc-plugin-markdown src/index.ts --out docs/api --includeVersion --readme ./docs/_api_readme.md"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "dts-bundle": "^0.7.3",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.1",
    "typedoc": "^0.24.7",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitepress": "1.0.0-beta.1"
  },
  "dependencies": {
    "merge-options": "^3.0.4",
    "pixi.js": "^7.2.4"
  }
}
